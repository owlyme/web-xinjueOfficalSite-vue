@font-face {
  font-family: 'iconfont';  /* project id 1047474 */
  src: url('//at.alicdn.com/t/font_1047474_hdm2sq7xbt8.eot');
  src: url('//at.alicdn.com/t/font_1047474_hdm2sq7xbt8.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1047474_hdm2sq7xbt8.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1047474_hdm2sq7xbt8.woff') format('woff'),
  url('//at.alicdn.com/t/font_1047474_hdm2sq7xbt8.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1047474_hdm2sq7xbt8.svg#iconfont') format('svg');
}
.icon {
  font-family: 'iconfont';
}
* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
  font-size: 12px
}
li {
  list-style: none;
}
a {
  text-decoration: none !important;
}
.center-middle {
  display: flex;
  justify-content: center;
  align-items: center;
}
.vertical{
  display: flex;
  align-items: center;
  justify-content: space-around;
  height: 100%;
  }
#app {
    background: #f5f5f5;
  }
.white
  color #ffffff
.gray
  color #f5f5f5
.red
  color #e63a3e;
.color333
  color #333
.color999
  color #999

.margin10
  margin 10px
.margin30
  margin 30px

.padding5
  padding 5px
.padding10
  padding 10px
.padding30
  padding 30px

.text0
  font-size: 18px;
  height 36px
.text1
  font-size: 13px;
  color: #999;
  line-height: 26px
.bgWhite
  background-color #ffffff
.bgGray
  background-color #f5f5f5

.font-weight
  font-size: 24px;
  font-weight: 700;
  line-height: 32px;
  color: #000;
.fnw500
  font-weight: 500;
.fnw100
  font-weight: 100;
.fn24
  font-size: 24px;

.line
  position: absolute;
  top 50%
  left 0
  height 1px
  border-top 1px solid #999



.visible-xs-block,
.visible-sm-block,
.visible-md-block,
.visible-lg-block
  display: none
@media (min-width: 1200px){
  .visible-lg-block {
    display: block!important;
  }
}
@media (min-width: 992px){
  .visible-md-block {
    display: block!important;
  }
}
@media (min-width: 768px){
  .visible-sm-block {
    display: block!important;
  }
}
